//1.Given a Point (x,y). Write a program to find whether it is on a line y=mx+c. 



import java.util.Scanner; 
public class FindOnline{ 

	public static void main(String args[]) 
	{	int m = 5; 
		int c = 1; 
		Scanner in = new Scanner(System.in); 
		int x= in.nextInt(); 
		int y= in.nextInt(); 
		if(y==(m*x + c)) 
			System.out.println("Yes. It is a point"); 
		else 
			System.out.println("No. It is not a point"); 
		 
	} 

